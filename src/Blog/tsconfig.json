{
  "compilerOptions": {
    "target": "es5",
    //"module": "system",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "removeComments": false,
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "noEmitOnError": true,
    "suppressImplicitAnyIndexErrors": true
    //"inlineSourceMap": true
    //"inlineSources": true
  },
  "exclude": [
    "wwwroot/lib",
    "node_modules",
    "typings/main",
    "typings/main.d.ts"
  ]
}