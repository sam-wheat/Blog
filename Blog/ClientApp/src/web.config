<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModule" resourceType="Unspecified" />
    </handlers>
    <aspNetCore processPath=".\Blog.exe" arguments="" stdoutLogEnabled="true" stdoutLogFile=".\logs\stdout">
      <environmentVariables>
        <environmentVariable name="ASPNETCORE_ENVIRONMENT" value="Production" />
      </environmentVariables>
    </aspNetCore>
    <rewrite>
      <rules>
        <rule name="About" stopProcessing="true">
          <match url="^About$" />
          <action type="Rewrite" url="/index.html" />
        </rule>
        <rule name="Contact" stopProcessing="true">
          <match url="^Contact$" />
          <action type="Rewrite" url="/index.html" />
        </rule>
        <rule name="BlogIndex" stopProcessing="true">
          <match url="^BlogIndex$" />
          <action type="Rewrite" url="/index.html" />
        </rule>
        <rule name="Post" stopProcessing="true">
          <match url="^Post/*" />
          <action type="Rewrite" url="/index.html" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>
